		<implementedBy>AVATAR_INFOS.avatar_info_inst</implementedBy>
		<Properties>
			<dbid>
				<Type>	DBID	</Type>
			</dbid>
			<name>
				<Type>	UNICODE	</Type>
				<DatabaseLength> 256 </DatabaseLength>
			</name>
			<roleType>
				<Type>	UINT8	</Type>
			</roleType>
			<gold>
				<Type>	UINT16	</Type>
			</gold>
      <seatIdx>
        <Type>	UINT8	</Type>
      </seatIdx>
      <isReady>
        <Type>	UINT8	</Type>
      </isReady>
      <winScore>
        <Type>	INT32	</Type>
      </winScore>
      <isRobot>
        <Type>	UINT8	</Type>
      </isRobot>
		</Properties>
	</AVATAR_INFOS>

	<AVATAR_INFOS_LIST>	FIXED_DICT
		<implementedBy>AVATAR_INFOS.AVATAR_INFOS_LIST_PICKLER</implementedBy>
		<Properties>
			<values>
				<Type>	ARRAY <of> AVATAR_INFOS </of>	</Type>
			</values>
		</Properties>
	</AVATAR_INFOS_LIST>

