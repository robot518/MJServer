      <seatIdx>
        <Type>	UINT8	</Type>
      </seatIdx>
      <playerName>
        <Type>	UNICODE	</Type>
      </playerName>
      <isOffline>
        <Type>	UINT8	</Type>
      </isOffline>
      <winScore>
        <Type>	INT32	</Type>
      </winScore>
		</Properties>
	</MJ_GAME_PLAYER>

	<MJ_GAME_PLAYER_LIST>	FIXED_DICT
		<implementedBy>MJ_GAME_PLAYER.MJ_GAME_PLAYER_LIST_PICKLER</implementedBy>
		<Properties>
			<values>
				<Type>	ARRAY <of> MJ_GAME_PLAYER </of>	</Type>
			</values>
		</Properties>
	</MJ_GAME_PLAYER_LIST>

