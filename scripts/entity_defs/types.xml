			</huaScore>
			<dictMeld>
				<Type>	ARRAY <of> MJ_MELD </of>	</Type>
			</dictMeld>
			<handTileStack>
				<Type>	ARRAY <of> UINT8 </of>	</Type>
			</handTileStack>
      <outputTiles>
				<Type>	ARRAY <of> UINT8 </of>	</Type>
			</outputTiles>
      <tilesCount>
        <Type>	UINT8	</Type>
